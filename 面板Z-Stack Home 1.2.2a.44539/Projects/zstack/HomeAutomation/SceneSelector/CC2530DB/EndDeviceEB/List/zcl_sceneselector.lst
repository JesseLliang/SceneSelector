   1074          static void zclSceneSelector_EZModeCB( zlcEZMode_State_t state, zclEZMode_CBData_t *pData )
   1075          {
   1076          #ifdef LCD_SUPPORTED
   1077            char szLine[20];
   1078            char *pStr;
